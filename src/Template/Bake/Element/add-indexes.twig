{% for indexName,index in indexes %}
    ->addIndex(
    [{% Migration.stringifyList(index.columns, {'indent': 5}) %}],
    [
    {% set params['name'] = indexName %}
    {% if (index.type === 'unique') %}
        {% set params['unique'] = true %}
    {% endif %}
    {% Migration.stringifyList(params, {'indent': 5}) %}
    ]
    )
{% endfor %}
